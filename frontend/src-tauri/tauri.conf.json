{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Bamboo Shoot Soup",
  "version": "0.1.0",
  "identifier": "com.awords.bambooshootsoup",
  "build": {
    "frontendDist": "../.output/public",
    "devUrl": "http://127.0.0.1:3000",
    "beforeDevCommand": "bun run dev:mobile",
    "beforeBuildCommand": "bun run generate"
  },
  "app": {
    "windows": [
      {
        "title": "Bamboo Shoot Soup",
        "width": 1280,
        "height": 860,
        "minWidth": 960,
        "minHeight": 680,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
